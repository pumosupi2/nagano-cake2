<div class='container px-5 px-sm-0'>
  <div class='row'>
    <div class='col-md-6'>
      <!--左-->
      <h4>ジャンル編集</h4>
      <br>
      <br>
        <div class="form-group">
          <!--<table>-->
          <!--tableタグを使うと反応しない-->
            <tbody>
            <%= form_with model: @genre,local:true, url: admin_genre_path, method: :patch do |f| %>
            <tr>
            <td>ジャンル名</td>
            <td><%= f.text_field :name, class: 'form-control genre_name' %></td>
            <td><%= f.submit "変更を保存",class: "btn btn-success" %></td>
            </tr>
            <% end %>
            </tbody>
          <!--</table>-->
        </div>
    </div>
    <!--右（空白）-->
    <div class='col-md-5 offset-md-1'>
    </div>
  </div>
</div>