<div class='container'>
  <div class='row'>
    <div>
       
    </div>
  </div>
  <div>
    <div>
    　<table>
        <tr>
         <td>会員ID</td>
         <td><%= @customer.id %></td>
        </tr>
        <tr>
         <td>氏名</td>
         <td><%= @customer.last_name %><%= @customer.first_name %></td>
        </tr>
        <tr>
         <td>フリガナ</td>
         <td><%= @customer.last_name_kana %><%= @customer.first_name_kana %></td>
        </tr>
        <tr>
         <td>郵便番号</td>
         <td><%= @customer.address %></td>
        </tr>
        <tr>
         <td>住所</td>
         <td><%= @customer.post_code %></td>
        </tr>
        <tr>
         <td>電話番号</td>
         <td><%= @customer.telephone_number %></td>
        </tr>
        <tr>
         <td>メールアドレス</td>
         <td><%= @customer.email %></td>
        </tr>
        <tr>
         <td>会員ステータス</td>
         <% if @customer.is_deleted == false %>
          <td>有効</td>
         <% else %>
          <td>退会</td>
         <% end %>
        </tr>
    　</table>
    </div>
  </div>
  <div>
      <%= link_to "編集する", edit_admin_customer_path(@customer) %>
  </div>
  <div>
      注文履歴一覧
  </div>
</div>